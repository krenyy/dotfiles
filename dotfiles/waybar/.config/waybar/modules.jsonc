{
  "clock": {
    "interval": 1,
    "format": "󰃰 {:%a %e. %b %Y - %H:%M}",
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
  },
  "cpu": {
    "interval": 1,
    "format": "  {usage}%",
  },
  "memory": {
    "interval": 1,
    "format": "󰍛 {}%",
  },
  "pulseaudio": {
    "format": "{icon} {volume}%",
    "format-icons": ["󰕿", "󰖀", "󰕾"],
    "ignored-sinks": ["virtual-sink"],
    "smooth-scrolling-threshold": 1.5,
  },
  "backlight": {
    "format": "{icon} {percent}%",
    "format-icons": ["", "", "", "", "", "", "", "", ""],
    "on-scroll-up": "brightnessctl s +1%",
    "on-scroll-down": "brightnessctl s 1%-",
    "smooth-scrolling-threshold": 2.5,
  },
  "battery": {
    "states": {
      "warning": 30,
      "critical": 15,
    },
    "format": "{icon} {capacity}%",
    "format-charging": "󰂄 {capacity}%",
    "format-plugged": " {capacity}%",
    "format-alt": "{time} {icon}",
    "format-icons": ["", "", "", "", ""],
  },
  "network#1": {
    "interface": "e0",
    "format-ethernet": "󰈀  {ifname}",
    "format-linked": "󰈀  {ifname}",
    "format-disconnected": "󰈀  {ifname}",
    "tooltip-format": "{ifname} via {ipaddr}/{cidr}",
  },
  "network#2": {
    "interface": "w0",
    "format-wifi": "  {essid} ({signalStrength}%)",
    "format-linked": "  {essid} ({signalStrength}%)",
    "format-disconnected": "  {ifname}",
    "tooltip-format": "{ifname} via {ipaddr}/{cidr}",
  },
  "bluetooth": {
    "format-connected-battery": " connected ({device_battery_percentage}%)",
    "format-disabled": "",
    "format-off": "",
    "interval": 30,
    "on-click": "footclient bluetuith",
  },
  "custom/power_draw": {
    "interval": 4,
    "exec": "$HOME/.config/waybar/power_draw.rs",
    "format": "󱐌{} W  ",
  },
  "custom/power_profile": {
    "interval": 1,
    "exec": "cat /sys/bus/platform/devices/tuxedo_platform_profile/platform_profile",
    "format": "󰓅 {}",
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "",
      "deactivated": "",
    },
  },
}
